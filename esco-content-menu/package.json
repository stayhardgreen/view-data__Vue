    "menu",
    "navigation"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "dist/esco-content-menu.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GIP-RECIA/uPortal-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/GIP-RECIA/uPortal-web-components/issues"
