{
  "name": "@gip-recia/esco-content-menu",
  "version": "0.2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --name esco-content-menu --target wc 'src/components/*.vue'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@uportal/open-id-connect": "^1.5.0",
    "datalist-polyfill": "^1.20.1",
    "vue": "^2.5.16",
    "vue-awesome": "^3.0.5",
    "vue-awesome-swiper": "^3.1.3",
    "vue-i18n": "^7.8.1",
    "vue-line-clamp": "^1.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.9",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.0.0-rc.9",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "vue-template-compiler": "^2.5.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "description": "Display a menu for a new kind of navigation: from favorites and browsable portlet list",
  "license": "Apache-2.0",
  "author": "Julien Gribonvald <julien.gribonvald@gmail.com>",
  "keywords": [
    "apereo",
    "uportal",
